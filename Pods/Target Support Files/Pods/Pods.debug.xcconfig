FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_FRAMEWORK_BUILD_PATH" "${PODS_ROOT}/Dropbox-iOS-SDK/dropbox-ios-sdk-1.3.13"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_FRAMEWORK_BUILD_PATH/Dropbox_iOS_SDK.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/GMImagePicker.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "DropboxSDK" -framework "GMImagePicker" -framework "QuartzCore" -framework "Security"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods
PODS_ROOT = ${SRCROOT}/Pods